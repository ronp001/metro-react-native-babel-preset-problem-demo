{
    "presets": [
        "module:metro-react-native-babel-preset"
    ],
    "plugins": [
        "@babel/plugin-transform-typescript",
        "@babel/plugin-transform-parameters",
        [
            "module-resolver",
            {
                "root": [
                    "./src"
                ],
                "extensions": [
                    ".ts",
                ],
                "cwd": "babelrc",
            }
        ]
    ]
}